{
    "apiVersion": 3,
    "name": "custom-block-package/map-block",
    "title": "Map Block",
    "category": "widgets",
    "icon": "location-alt",
    "description": "A block to display an interactive map using Leaflet.js.",
    "supports": {
        "html": false
    },
    "attributes": {
        "latitude": {
            "type": "number",
            "default": 50.031562
        },
        "longitude": {
            "type": "number",
            "default": 21.997937
        },
        "zoom": {
            "type": "number",
            "default": 13
        },
        "containerHeight": {
            "type": "number",
            "default": 300
        }
    },
    "editorScript": "file:./index.js",
    "viewScript": "file:./frontend.js",
    "editorStyle": "file:./index-rtl.css",
    "viewStyle": "file:./style-index.css",
    "render": "file:./render.php"
}