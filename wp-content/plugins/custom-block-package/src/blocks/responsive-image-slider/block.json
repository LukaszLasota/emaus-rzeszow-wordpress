{
  "apiVersion": 3,
  "name": "custom-block-package/responsive-image-slider",
  "title": "Responsywny slider",
  "category": "custom-blocks-from-scratch",
  "attributes": {
    "desktopImages": {
      "type": "array",
      "default": []
    },
    "mobileImages": {
      "type": "array",
      "default": []
    },
    "mobileBreakpoint": {
      "type": "number",
      "default": 767
    }
  },
  "editorScript": [
    "file:./index.js"
  ],
  "editorStyle": [
    "glide-core-style",
    "file:./index.css"
  ],
  "viewScript": [
    "glide-init-script",
    "file:./frontend.js"
  ],
  "viewStyle": [
    "glide-core-style",
    "glide-theme-style",
    "file:./style-index.css"
  ],
  "render": "file:./render.php"
}
