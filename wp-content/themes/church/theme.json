{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"patterns": [
		"three-columns-of-services",
		"clients-section"
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"duotone": [
				{
					"colors": ["#111111", "#ffffff"],
					"slug": "duotone-1",
					"name": "Black and white"
				}
			],
			"gradients": [
				{
				  "slug": "gradient-to-left-bottom",
				  "gradient": "linear-gradient(to left bottom, #008089 0%, #002432 100%)",
				  "name": "Gradient to Left Bottom"
				},
				{
				  "slug": "gradient-to-bottom",
				  "gradient": "linear-gradient(to bottom, #008089 0%, #002432 100%)",
				  "name": "Gradient to Bottom"
				},
				{
				  "slug": "gradient-90-deg",
				  "gradient": "linear-gradient(90deg,#008089 0,#002432)",
				  "name": "Gradient 90 deg"
				}
			],
			"palette": [
				{
					"color": "#f9f9f9",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#ffffff",
					"name": "Base / Two",
					"slug": "base-2"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'Raleway', sans-serif",
					"name": "Raleway",
					"slug": "raleway"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.9rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.05rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.39rem",
						"max": "1.85rem"
					},
					"name": "Large",
					"size": "1.85rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.85rem",
						"max": "2.5rem"
					},
					"name": "Extra Large",
					"size": "2.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.5rem",
						"max": "3.27rem"
					},
					"name": "Extra Extra Large",
					"size": "3.27rem",
					"slug": "xx-large"
				}
			],
			"writingMode": true
		},
		"custom": {
			"content-width-mobile": "90%",
			"content-width-tablet": "80%",
			"content-width-desktop": "70%",
			"content-width-large-desktop": "60%"
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--raleway)"
		}
	}

}
